<div style='width: 800px;'>
  <div id="geolocation" style='width: 1270px; height: 500px;'></div>
</div>
<script type="text/javascript">
  handler = Gmaps.build('Google');
  handler.buildMap({ provider: {}, internal: {id: 'geolocation'}}, function(){
  marker1 = handler.addMarkers(<%=raw @hashs.to_json %>);
  marker2 = handler.addMarkers(<%=raw @hash.to_json %>);
  handler.bounds.extendWith(marker1);
  handler.bounds.extendWith(marker2);
  handler.fitMapToBounds();
  });
</script>
